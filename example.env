# OpenAI API Key for authenticating requests to the OpenAI API
OPENAI_API_KEY=

# Language preference for OpenAI responses (e.g., German, English)
OPENAI_LANGUAGE=German

# OpenAI model to use for generating titles (e.g., gpt-4.1 or gpt-4o-mini)
OPENAI_MODEL=gpt-4.1
# Alternative OpenAI model (uncomment to use)
# OPENAI_MODEL=gpt-4o-mini

# Mistral API Key for authenticating requests to the Mistral OCR API
MISTRAL_API_KEY=

# Paperless-ngx API Key for accessing the Paperless-ngx instance
PAPERLESS_API_KEY=

# Base URL of the Paperless-ngx instance (e.g., http://localhost:8000)
PAPERLESS_BASE_URL=

# Tag ID in Paperless-ngx for documents that require title generation
PAPERLESS_GENERATE_TITLE_TAG_ID=

# Tag ID in Paperless-ngx for documents that require OCR processing
PAPERLESS_GENERATE_OCR_TAG_ID=